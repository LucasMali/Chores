            <Label fx:id="memberNameLabel" text="Name">
               <HBox.margin>
                  <Insets right="15.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="memberNameInput" prefHeight="25.0" prefWidth="337.0" />
