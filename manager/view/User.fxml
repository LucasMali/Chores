<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="192.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="manager.view.UserController">
   <top>
      <TextFlow prefHeight="80.0" prefWidth="600.0" BorderPane.alignment="CENTER">
